{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "env-cmd -f ./.env nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "concurrently \"npm run client-install\" \"npm install\" \"npm run build\"",
    "build": "npm run build --prefix client"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "env-cmd": "^10.0.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.4.1",
    "mongoose": "^5.8.4",
    "nodemon": "^1.19.1",
    "react-router-dom": "^5.1.2",
    "validator": "^12.1.0"
  }
}
